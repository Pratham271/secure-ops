{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"],
      "env": [
        "DATABASE_URL",
        "NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY",
        "CLERK_SECRET_KEY",
        "NEXT_PUBLIC_CLERK_SIGN_IN_URL",
        "NEXT_PUBLIC_CLERK_SIGN_UP_URL",
        "GITHUB_APP_SLUG",
        "GITHUB_APP_CLIENT_ID",
        "GITHUB_APP_CLIENT_SECRET",
        "GITHUB_APP_ID",
        "GITHUB_PRIVATE_KEY",
        "GROQ_API_KEY",
        "PRIMARY_MODEL",
        "FALLBACK_MODEL",
        "ARCHESTRA_PROXY_URL",
        "ARCHESTRA_CHAT_URL",
        "ARCHESTRA_PROMETHEUS_URL",
        "COST_LIMIT_DAILY",
        "TEAM_NAME",
        "NODE_ENV",
        "PORT",
        "NEXTAUTH_URL",
        "NEXTAUTH_SECRET",
        "ENCRYPTION_KEY",
        "VERCEL"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "start": {
      "cache": false,
      "persistent": true
    },
    "lint": {
      "outputs": []
    },
    "type-check": {
      "outputs": []
    },
    "clean": {
      "cache": false
    }
  }
}
